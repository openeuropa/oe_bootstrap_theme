{#
/**
 * @file
 * Template for a unformatted rows.
 */
#}
{% if title %}
  <h3>{{ title }}</h3>
{% endif %}
<div class="row row-cols-1 g-4">
{% for row in rows %}
  {%
    set row_classes = [
      'col',
      default_row_class ? 'views-row',
    ]
  %}
  <div{{ row.attributes.addClass(row_classes) }}>
    {{- row.content -}}
  </div>
{% endfor %}
</div>
